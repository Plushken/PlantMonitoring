{
  "expo": {
    "name": "Plantinca",
    "slug": "plantinca-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "scheme": "plantinca",
    "platforms": ["ios", "android", "web"],
    "splash": {
      "image": "./assets/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#F0F8FF"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#F0F8FF"
      }
    },
    "web": {
      "favicon": "./assets/icon.png"
    }
  }
}
