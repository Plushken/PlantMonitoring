{
  "name": "plantmonitoring",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "android-development": "DEBUG=expo:* npx expo run:android --variant debug",
    "android-production": "DEBUG=expo:* npx expo run:android --variant release",
    "format": "prettier . --log-level debug --write",
    "ios-development": "DEBUG=expo:* npx expo run:ios --configuration Debug",
    "ios-production": "DEBUG=expo:* npx expo run:ios --configuration Release",
    "lint": "DEBUG=expo:* npx expo lint --fix",
    "prebuild": "DEBUG=expo:* npx expo prebuild --platform ios",
    "start": "DEBUG=expo:* npx expo start",
    "update": "npx npm-check-updates -u --verbose",
    "android": "expo run:android",
    "ios": "expo run:ios"
  },
  "dependencies": {
    "@expo/metro-config": "~0.20.0",
    "@react-native-async-storage/async-storage": "2.1.2",
    "expo": "^53.0.20",
    "expo-image": "~2.3.2",
    "expo-image-picker": "~16.1.4",
    "expo-linking": "~7.1.7",
    "expo-network": "^7.1.5",
    "expo-router": "~5.1.4",
    "react": "19.0.0",
    "react-native": "0.79.5",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-webview": "13.13.5",
    "expo-constants": "~17.1.7",
    "expo-status-bar": "~2.2.3"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@types/react": "~19.0.10",
    "@types/react-native": "^0.72.8",
    "typescript": "~5.8.3",
    "eslint": "^9.0.0",
    "eslint-config-expo": "~9.2.0"
  },
  "private": true
}
